<script>

    function checkForExistingUser(username,  password, type) {

        if(username.length == 0){
            alertCustomMessage("Prašome įvesti vartotojo vardą.");
            return;
        }
        if(password == ""){
            alertCustomMessage("Prašome įvesti slaptažodį.");
            return;
        }

        var rawFile = new XMLHttpRequest();
        var userFound = false;


        if(type == "student") {
            rawFile.open("GET", "/javascripts/data/users/users-student.txt", true);
            rawFile.onreadystatechange = function () {
                if (rawFile.readyState === 4) {
                    var allText = rawFile.responseText;
                    var arrayOfLines = allText.match(/[^\r\n]+/g);
                    for (var i = 0; i < arrayOfLines.length; i++) {
                        var userData = arrayOfLines[i].split(" ");
